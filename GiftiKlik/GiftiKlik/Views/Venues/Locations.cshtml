@model List<GiftiKlik.Models.VenuesViewModel>



<table border="1" class="table table-striped">
    <tr>
        <th>Име</th>
        <th>Улица</th>
        <th>Број</th>
        <th>Град</th>
        <th></th>
    </tr>

    @foreach (var item in @Model)
    {
        <tr>
            <td>
                @item.Name
            </td>
            <td>
                @item.Street
            </td>
            <td>
                @item.StreetNumber
            </td>

            <td>
                @item.CityName
            </td>

            <td>
                @Html.ActionLink("Одбери", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Детали", "Details", new { /* id=item.PrimaryKey */ }) |

            </td>
           
        </tr>
    }
</table>
<a href="/home/friends" class="btn btn-primary btn-lg">Назад</a>


